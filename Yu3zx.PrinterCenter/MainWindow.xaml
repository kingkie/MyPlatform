<Window
    x:Class="Yu3zx.PrinterCenter.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Yu3zx.PrinterCenter"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="1080"
    Height="1080"
    mc:Ignorable="d">
    <Grid>
        <ScrollViewer x:Name="fullContent">
            <Grid
                x:Name="gridContent"
                Width="1080"
                HorizontalAlignment="Center">
                <StackPanel>
                    <TextBlock />
                </StackPanel>
            </Grid>
        </ScrollViewer>
    </Grid>
</Window>
